{
  "expo": {
    "name": "VineHealth",
    "slug": "client",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/vineHealth.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/vineHealth-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#8E97FD"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/vineHealth.png",
        "backgroundColor": "#8E97FD"
      },
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.alvinle29.vineHealth"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "734c11c0-a754-43a5-9161-594b3c74c39a"
      }
    }
  }
}
